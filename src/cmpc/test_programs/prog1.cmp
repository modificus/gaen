
component Comp1
{
    int #propInt;
    float #propFloat;
    int #propOtherEntity;

    #message1(int i, float f)
    {
        @:Comp1#propInt = i;
        @:Comp1#propFloat = f;

        @:Comp1#message2(false);
    }

    #message2(bool b)
    {
        if (b)
        {
            @:#message1(20, 30.0);
        }
        else
        {
            @propOtherEntity:#testMessage(10, 20);
            @1234:#testMessage(10, 20);
        }
    }

    #update(float deltaSecs)
    {
        @:Comp1#propFloat = deltaSecs;
    }
}

